{
  "name": "english-studying",
  "version": "0.3.0",
  "description": "英語、応用情報技術者試験、高度情報技術者試験、G検定の学習用語句を効率的に管理・復習するためのWebアプリケーションです。",
  "private": true,
  "scripts": {
    "start": "NODE_OPTIONS=--max_old_space_size=4096 react-scripts start",
    "start:fast": "GENERATE_SOURCEMAP=false ESLINT_NO_DEV_ERRORS=true TSC_COMPILE_ON_ERROR=true react-scripts start",
    "start:light": "NODE_OPTIONS=--max_old_space_size=2048 GENERATE_SOURCEMAP=false FAST_REFRESH=false TSC_COMPILE_ON_ERROR=true ESLINT_NO_DEV_ERRORS=true react-scripts start",
    "start:mem": "NODE_OPTIONS=--max_old_space_size=4096 GENERATE_SOURCEMAP=false ESLINT_NO_DEV_ERRORS=true TSC_COMPILE_ON_ERROR=true react-scripts start",
    "dev": "npm run start:fast",
    "dev:with-server": "concurrently \"npm run server\" \"npm start\"",
    "server": "node server.js",
    "build": "react-scripts build",
    "start:prod": "npm run build && npx serve -s build -l 3000",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf node_modules/.cache",
    "firebase:init-client": "node scripts/firebase_init_client.js",
    "firebase:migrate": "node scripts/mysql_to_firestore.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yusei-Maekawa/English-Studying.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yusei-Maekawa/English-Studying/issues"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "mysql2": "^3.14.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "^5.0.1",
    "sanitize-html": "^2.17.0",
    "typescript": "^4.9.5",
    "express-rate-limit": "^8.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/sanitize-html": "^2.16.0",
    "concurrently": "^7.6.0",
    "firebase": "^10.10.0",
    "firebase-admin": "^13.5.0",
    "mysql2": "^3.14.3"
  }
}
